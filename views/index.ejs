<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <table border="1">
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Create</th>
        <th>Completed</th>
      </tr>
      <% for(var i = 0; i < todoData.length; i++){ %>
        <tr>
          <td><%= todoData[i].id %></td>
          <td><%= todoData[i].title %></td>
          <td><%= todoData[i].createdAt %></td>
          <td><%= todoData[i].is_complete %></td>
        </tr>
      <% } %>
    </table>
  </body>
</html>
