<% include ../index %>
<h1><%= title %></h1>
<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Complete</th>
  </tr>
  <% for(var i = 0; i < todoData.length; i++){ %>
    <tr>
      <td><%= todoData[i].id %></td>
      <td><%= todoData[i].title %></td>
      <td><%= todoData[i].is_complete %></td>
    </tr>
  <% } %>
</table>
